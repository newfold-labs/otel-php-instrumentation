{
  "name": "newfold-labs/otel-php-instrumentation",
  "description": "OpenTelemetry auto-instrumentation for Custom PHP",
  "keywords": [
    "opentelemetry",
    "otel",
    "open-telemetry",
    "tracing",
    "supersite",
    "instrumentation",
    "Custom PHP"
  ],
  "type": "library",
  "readme": "./README.md",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "version": "1.0.0",
  "authors": [
    {
      "name": "Mayur Saptal",
      "email": "mayur.saptal@newfold.com"
    },
    {
      "name": "Suraj Rathod",
      "email": "suraj.rathod@newfold.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-opentelemetry": "*",
    "open-telemetry/api": "^1.0",
    "open-telemetry/sem-conv": "^1.30",
    "nyholm/psr7": "^1",
    "nyholm/psr7-server": "^1",
    "open-telemetry/sdk": "^1.2",
    "open-telemetry/exporter-otlp": "^1.1",
    "open-telemetry/opentelemetry-auto-psr18": "^1.0",
    "symfony/http-client": "^6.4",
    "guzzlehttp/promises": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3",
    "phan/phan": "^5.0",
    "php-http/mock-client": "*",
    "phpstan/phpstan": "^1.1",
    "phpstan/phpstan-phpunit": "^1.0",
    "psalm/plugin-phpunit": "^0.19.2",
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "6.4.0"
  },
  "autoload": {
    "psr-4": {
      "NewfoldLabs\\CustomPhp\\Instrumentation\\": "src/"
    },
    "files": [
      "_register.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "NewfoldLabs\\CustomPhp\\Instrumentation\\": "tests/"
    }
  },
  "config": {
    "platform": {
      "php": "8.1.32"
    },
    "allow-plugins": {
      "php-http/discovery": true,
      "tbachert/spi": true
    }
  }
}